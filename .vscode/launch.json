{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch debug.",
			"preLaunchTask": "Build GCC debug.",
			"type": "cppdbg",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/bag.bin",
			"args": [],
			"stopAtEntry": false,
			"externalConsole": false,
			"miDebuggerPath": "gdb",
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}
